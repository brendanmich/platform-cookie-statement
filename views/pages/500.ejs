<!doctype HTML>

<html lang="en">
	<head>

		<!-- Google Tag Manager -->
		<% if (process.env.ENV === "production") { %>
			<%- include('../components/google_tag_head')  %>
			<% if (state.consent.analytics_enabled) { %><%- include('../components/hotjar_head') %><% } %>
		<% } %>

		<!-- Site Meta -->
		<title>Page Not Found</title>
		<meta name="description" content="Uh oh, we can’t seem to find the page you’re looking for.">
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width" />
		<meta name="robots" content="noindex" />

		<!-- External Fonts -->
		<%- include('../components/fonts') %>

		<!-- reCAPTCHA -->
		<% if (state.consent.performance_enabled) { %><%- include('../components/recaptcha') %><% } %>

		<!-- Styles -->
		<link rel="stylesheet" type="text/css" href="<%= process.env.CDN_URL %>/public/<%= webpack_asset('error_500','css') %>">

		<!-- Favicon -->
		<link rel="shortcut icon" href="<%= process.env.CDN_URL %>/public/img/favicon.ico" type="image/x-icon">
		<link rel="icon" href="<%= process.env.CDN_URL %>/public/img/favicon.ico" type="image/x-icon">
	</head>

	<body class="preload hide-captcha">

		<!-- Google Tag Manager -->
		<% if (process.env.ENV === "production") { %>
			<%- include('../components/google_tag_body') %>
		<% } %>

		<!-- Header -->
		<%- include('../navigation/header.ejs') %>

		<!-- Content -->
		<div class="error-page"></div>

		<!-- Footer -->
		<%- include('../navigation/footer') %>

		<!-- Cookie Bar -->
		<%- include('../components/cookie_bar') %>

		<!-- Scripts -->
		<script src="<%= process.env.CDN_URL %>/public/<%= webpack_asset('error_500','js') %>"></script>
	</body>
</html>
